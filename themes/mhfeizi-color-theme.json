{
  "name": "mhfeizi",
  "type": "dark",
  "colors": {
    "editor.background": "#0a0e0f",
    "sideBar.background": "#111516",
    "activityBar.background": "#0c1011",
    "statusBar.background": "#111516",
    "menu.background": "#0c1011",
    "titleBar.activeBackground": "#0c1011",
    "tab.activeBackground": "#0c1011",
    "tab.inactiveBackground": "#0c1011",
    "editor.lineHighlightBorder": "#0c1011",
    "terminal.foreground": "#fff",
    "terminal.background": "#000",
    "terminal.ansiBlack": "#000",
    "terminal.ansiRed": "#ff0000",
    "terminal.ansiGreen": "#05F605",
    "terminal.ansiYellow": "#ff0",
    "terminal.ansiBlue": "#025ee7",
    "terminal.ansiCyan": "#0ff",
    "terminal.ansiMagenta": "#ce03ce",
    "terminal.ansiWhite": "#fff",
    "terminal.ansiBrightBlack": "#696868",
    "terminal.ansiBrightRed": "#ff0000",
    "terminal.ansiBrightGreen": "#05F605",
    "terminal.ansiBrightYellow": "#ff0",
    "terminal.ansiBrightBlue": "#025ee7",
    "terminal.ansiBrightCyan": "#0ff",
    "terminal.ansiBrightMagenta": "#ce03ce",
    "terminal.ansiBrightWhite": "#a3a3a3"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#555454"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#c6dce0"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#0e8504"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#f73757"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#07a34d"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.separator.inheritance.php",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#04c0eb"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#f301a2"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "meta.tag.sgml", "markup.deleted.git_gutter"],
      "settings": {
        "foreground": "#04c0eb"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": ["entity.name.function", "meta.function-call", "variable.function", "support.function", "keyword.other.special-method"],
      "settings": {
        "foreground": "#bff803"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#c6dce0"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#f5077e"
      }
    },
    {
      "name": "Constant, Tag Attribute, Embedded",
      "scope": ["constant.language", "support.constant", "keyword.other.unit", "keyword.other", "constant.character", "constant.escape"],
      "settings": {
        "foreground": "#f8fc02"
      }
    },
    {
      "name": "Number,Function Argument",
      "scope": ["constant.numeric", "variable.parameter"],
      "settings": {
        "foreground": "#04c0eb"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#05ee11"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#f7034c"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#f7ab06"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#f7047e"
      }
    },
    {
      "name": "Sub-methods",
      "scope": ["entity.name.module.js", "variable.import.parameter.js", "variable.other.class.js"],
      "settings": {
        "foreground": "#e00245"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#e00245"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#e00245"
      }
    },
    {
      "name": "meta.method.js",
      "scope": ["meta.class-method.js entity.name.function.js", "variable.function.constructor"],
      "settings": {
        "foreground": "#e00245"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#fc06aa"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": ["text.html.basic entity.other.attribute-name.html", "text.html.basic entity.other.attribute-name"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#fc06aa"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#eef103"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#f7047e"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#03bcf5"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#adadad"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": ["tag.decorator.js entity.name.tag.js", "tag.decorator.js punctuation.definition.tag.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": ["source.js constant.other.object.key.js string.unquoted.label.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
